{
  "name": "jump-to-next-assignment",
  "displayName": "Jump To Next Assignment",
  "description": "Jump to the next line where the selected variable is assigned",
  "icon": "img/logo.png",
  "version": "2.0.1",
  "publisher": "bmpenuelas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bmpenuelas/jump-to-next-assignment-vscode.git"
  },
  "engines": {
    "vscode": "^1.65.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jumpToNextAssignment.jumpToNextAssignment",
        "title": "Jump to next assignment"
      }
    ],
    "keybindings": [
      {
        "command": "jumpToNextAssignment.jumpToNextAssignment",
        "key": "alt+a",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Jump To Next Assignment",
      "properties": {
        "jumpToNextAssignment.allowCharsBeforeAssignOp": {
          "type": "boolean",
          "default": true,
          "description": "Allow characters between variable name and assignment operator"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.65.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
